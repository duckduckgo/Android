.pagination a:hover, .pagination a:focus, .pagination span:hover, .pagination span:focus, .pagination em:hover, .pagination em:focus { text-decoration: none; border-color: var(--borderColor-default, var(--color-border-default)); transition-duration: 0.1s; }

