    <!-- New Onboarding Experience -->
    <string name="onboardingWelcomeTitle">Velkommen til DuckDuckGo!</string>
    <string name="onboardingDaxText">Internettet kan være ret uhyggeligt.&lt;br/&gt;&lt;br/&gt;Bare rolig! Det er lettere at søge og browse privat, end du tror.</string>
    <string name="onboardingLetsDoItButton">Lad os gøre det!</string>
    <string name="daxIntroCtaText">Prøv derefter at besøge et af dine foretrukne websteder! &lt;br/&gt;&lt;br/&gt; Den blokerer trackere, så de ikke kan spionere dig. Den opgraderer også din forbindelsessikkerhed, hvis det er muligt. &#128274;</string>
    <string name="daxEndCtaText">Du har det! &lt;br/&gt;&lt;br/&gt;Husk: hver gang du browser med mig, mister en uhyggelig annonce sine vinger. &#128077;</string>
