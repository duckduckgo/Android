{
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["ES2015"],
    "module": "esnext"
  },
  "include": ["."]
}
