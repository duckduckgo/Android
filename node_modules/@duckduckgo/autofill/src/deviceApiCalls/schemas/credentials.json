{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Credentials",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "description": "If present, must be a string",
      "type": "string"
    },
    "username": {
      "description": "This field is always present, but sometimes it could be an empty string",
      "type": "string"
    },
    "password": {
      "type": "string"
    }
  },
  "required": [
    "username",
    "password"
  ]
}
