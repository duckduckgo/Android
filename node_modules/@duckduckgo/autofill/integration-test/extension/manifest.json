{
  "name": "integration-test-extension",
  "description": "integration-test-extension",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
      "scripts": [
          "background.js"
      ]
  },
  "permissions": [
      "<all_urls>",
      "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["autofill.js"],
      "all_frames": true,
      "match_about_blank": true
    }
  ],
  "web_accessible_resources": [
      "public/css/autofill.css",
      "img/logo-small.svg",
      "autofill.js"
  ]
}
