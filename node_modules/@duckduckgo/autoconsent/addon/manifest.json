{
  "manifest_version": 3,
  "name": "Autoconsent",
  "version": "2022.5.18",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "cookies",
    "tabs",
    "scripting",
    "storage",
    "webNavigation"
  ],
  "action": {
    "browser_style": true,
    "default_title": "Autoconsent"
  }
}