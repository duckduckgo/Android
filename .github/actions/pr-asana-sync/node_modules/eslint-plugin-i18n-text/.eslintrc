{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "camelcase": "error",
    "eqeqeq": ["error", "smart"],
    "no-implicit-globals": "error",
    "no-unused-expressions": "error",
    "no-var": "error",
    "prefer-const": "error",
    "strict": "error"
  }
}
