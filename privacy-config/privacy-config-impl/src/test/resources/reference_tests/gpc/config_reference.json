{
    "readme": "This configuration has gpc feature only with some exceptions",
    "features": {
        "gpc": {
            "state": "enabled",
            "exceptions": [
                {
                    "domain": "washingtonpost.com",
                    "reason": "This is a domain where our iOS/Android apps inject the Sec-GPC request header"
                }
            ],
            "settings": {
                "gpcHeaderEnabledSites": [
                    "global-privacy-control.glitch.me",
                    "washingtonpost.com",
                    "globalprivacycontrol.org",
                    "example.com"
                ]
            }
        }
    },
    "version": 1635943904459,
    "unprotectedTemporary": [
        {
            "domain": "globalprivacycontrol.org",
            "reason": "This exception should disable all features"
        }
    ]
}